{
  "compilerOptions": {
    "module": "commonjs",
    "skipLibCheck": true,
    "target": "ES2024",
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "generate",
    "outDir": "dist"
  },
  "include": ["generate/*.ts", "generate/vscode-icons/src/defaults/fileIcons.ts"],
  "exclude": ["generate/vscode-icons/*.ts"]
}
