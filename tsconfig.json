{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2023",
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "outDir": "dist"
  },
  "include": ["generate/*.ts", "generate/vscode-icons/src/defaults/fileIcons.ts"],
  "exclude": ["generate/vscode-icons/*.ts"]
}
